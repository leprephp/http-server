{
    "name": "lepre/http-server",
    "description": "An implementation of PSR-15 HTTP Server Request Handlers",
    "keywords": [
        "server",
        "http server",
        "server request handlers",
        "middleware",
        "server middleware",
        "psr-15",
        "psr-7"
    ],
    "homepage": "https://github.com/leprephp/http-server",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniele De Nobili",
            "email": "danieledenobili@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/leprephp/http-server",
        "issues": "https://github.com/leprephp/http-server/issues"
    },
    "require": {
        "php": "^7.1",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0",
        "php-coveralls/php-coveralls": "^2.1",
        "zendframework/zend-diactoros": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Lepre\\Http\\Server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lepre\\Http\\Server\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
